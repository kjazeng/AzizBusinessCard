{
    "env": {
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "quotes": ["error", "double"],
      "semi": ["error", "always"],
      "indent": ["error", 2],
      "max-len": "off"
    }
  }
  